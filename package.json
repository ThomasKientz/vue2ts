{
  "name": "boomerang",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open --port 8050",
    "dev": "npm run serve",
    "build": "vue-cli-service build",
    "build-local": "vue-cli-service build --mode localhost && npx cap copy",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "upgrade-interactive": "npm-check --update"
  },
  "main": "background.js",
  "dependencies": {
    "@capacitor/android": "^1.3.0",
    "@capacitor/cli": "^1.3.0",
    "@capacitor/core": "^1.3.0",
    "@capacitor/ios": "^1.3.0",
    "@ionic-native/app-preferences": "^5.17.0",
    "@ionic-native/core": "^5.17.0",
    "axios": "^0.19.0",
    "cordova-plugin-app-preferences": "^0.99.3",
    "core-js": "^2.6.5",
    "date-fns": "^2.8.1",
    "electron-positioner": "^4.1.0",
    "electron-store": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.10",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.1.1"
  },
  "devDependencies": {
    "@mdi/js": "^4.5.95",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "deepmerge": "^4.2.2",
    "electron": "^6.0.10",
    "electron-icon-builder": "^1.0.2",
    "electron-notarize": "^0.2.1",
    "eslint": "^6.6.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "fibers": "^4.0.2",
    "menubar": "^6.0.8",
    "npm-check": "^5.9.0",
    "sass": "^1.23.6",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-electron-builder": "^1.4.1",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "webpack-conditional-loader": "^1.0.12"
  }
